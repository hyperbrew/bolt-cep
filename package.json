{
  "name": "bolt-cep",
  "version": "0.0.4",
  "scripts": {
    "dev": "vite --mode developemnt",
    "debug": "cross-env DEBUG_REACT=true vite build --watch true",
    "tools": "react-devtools",
    "watch": "tsc && vite build --watch true",
    "build": "rimraf dist/* && tsc && vite build --watch false",
    "build-vue": "rimraf dist/* && vue-tsc --isolatedModules false && vite build",
    "zxp": "tsc && cross-env ZXP_PACKAGE=true vite build --watch false",
    "serve": "cross-env SERVE_PANEL=true vite preview"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^3.10.0",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.7",
    "@vitejs/plugin-react": "^1.3.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "babel-plugin-react-require": "^3.1.3",
    "babel-plugin-transform-scss": "^1.1.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "prettify-xml": "^1.2.0",
    "react-devtools": "^4.22.1",
    "react-require": "^1.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-node-copy": "^1.0.4",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.43.4",
    "types-for-adobe": "^7.0.0",
    "types-for-adobe-extras": "^0.0.1",
    "typescript": "^4.4.4",
    "vite": "^2.9.1",
    "vite-cep-plugin": "^0.1.1",
    "vue-tsc": "^0.29.8",
    "yarn": "^1.22.17"
